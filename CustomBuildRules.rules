<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="Client custom build rules"
	Version="8.00"
	>
	<Rules>
		<CustomBuildRule
			Name="PublishDLLDependency"
			CommandLine="copy &quot;$(InputPath)&quot; &quot;$(TargetDir)&quot;"
			Outputs="&quot;$(TargetDir)$(InputFileName)&quot;"
			FileExtensions="*.dll;*.pdb"
			ExecutionDescription="Copying $(InputPath)"
			>
			<Properties>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
